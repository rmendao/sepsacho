<p id="notice"><%= notice %></p>

<h1>Listing Alarme Silos</h1>

<table>
  <thead>
    <tr>
      <th>Alarme</th>
      <th>Silo</th>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alarme_silos.each do |alarme_silo| %>
      <tr>
        <td><%= alarme_silo.alarme %></td>
        <td><%= alarme_silo.silo %></td>
        <td><%= alarme_silo.tipo %></td>
        <td><%= link_to 'Show', alarme_silo %></td>
        <td><%= link_to 'Edit', edit_alarme_silo_path(alarme_silo) %></td>
        <td><%= link_to 'Destroy', alarme_silo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Alarme silo', new_alarme_silo_path %>
